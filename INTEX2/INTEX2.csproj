        <ImplicitUsings>enable</ImplicitUsings>
        <UserSecretsId>aspnet-INTEX2-94BB59C7-F605-4E49-ABF5-1B506A52781D</UserSecretsId>
    </PropertyGroup>

    <ItemGroup>
        <None Update="app.db" CopyToOutputDirectory="PreserveNewest" ExcludeFromSingleFile="true"/>
